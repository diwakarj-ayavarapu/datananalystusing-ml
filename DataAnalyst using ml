        "project_ideas = [\n",
        "    {\n",
        "        \"name\": \"Analyze E-commerce Sales Data\",\n",
        "        \"description\": \"Clean, explore, and visualize sales data to identify trends, top-performing products, and customer behavior.\",\n",
        "        \"complexity\": \"Beginner\",\n",
        "        \"domain\": \"E-commerce\",\n",
        "        \"skills\": [\"Data Cleaning\", \"Visualization\", \"Exploratory Data Analysis\"]\n",
        "    },\n",
        "    {\n",
        "        \"name\": \"Predict Housing Prices\",\n",
        "        \"description\": \"Use regression techniques to build a model that predicts housing prices based on various features like location, size, and number of rooms.\",\n",
        "        \"complexity\": \"Intermediate\",\n",
        "        \"domain\": \"Real Estate\",\n",
        "        \"skills\": [\"Data Cleaning\", \"Feature Engineering\", \"Machine Learning (Regression)\", \"Model Evaluation\"]\n",
        "    },\n",
        "    {\n",
        "        \"name\": \"Analyze Social Media Sentiment\",\n",
        "        \"description\": \"Collect social media data (e.g., tweets), perform sentiment analysis, and visualize the results to understand public opinion on a specific topic or product.\",\n",
        "        \"complexity\": \"Intermediate\",\n",
        "        \"domain\": \"Social Media\",\n",
        "        \"skills\": [\"Data Collection (API usage)\", \"Text Preprocessing\", \"Sentiment Analysis\", \"Visualization\"]\n",
        "    },\n",
        "    {\n",
        "        \"name\": \"Build a Recommendation System\",\n",
        "        \"description\": \"Develop a recommendation system (e.g., collaborative filtering or content-based) using a dataset of user ratings or interactions.\",\n",
        "        \"complexity\": \"Advanced\",\n",
        "        \"domain\": \"Various (e.g., E-commerce, Entertainment)\",\n",
        "        \"skills\": [\"Data Cleaning\", \"Feature Engineering\", \"Machine Learning (Recommendation Systems)\", \"Model Evaluation\"]\n",
        "    },\n",
        "    {\n",
        "        \"name\": \"Analyze Healthcare Readmissions\",\n",
        "        \"description\": \"Analyze healthcare data to identify factors contributing to hospital readmissions and potentially build a predictive model.\",\n",
        "        \"complexity\": \"Intermediate\",\n",
        "        \"domain\": \"Healthcare\",\n",
        "        \"skills\": [\"Data Cleaning\", \"Exploratory Data Analysis\", \"Statistical Modeling\", \"Handling Imbalanced Data\"]\n",
        "    }\n",
        "]\n",
        "\n",
        "for idea in project_ideas:\n",
        "    print(f\"Project Name: {idea['name']}\")\n",
        "    print(f\"Description: {idea['description']}\")\n",
        "    print(f\"Complexity: {idea['complexity']}\")\n",
        "    print(f\"Domain: {idea['domain']}\")\n",
        "    print(f\"Skills: {', '.join(idea['skills'])}\")\n",
        "    print(\"-\" * 20)"
